    {
      "Name": "播報員",
      "Text": "強者們在府中的東京賽場齊聚一堂！\n究竟誰能奪得勝利的榮耀呢！\n「天皇賞（秋）」即將開始！",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "獨白",
      "Text": "（哇啊啊啊啊──────！！！！）",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "管家婆婆",
      "Text": "阿爾丹小姐……",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "？？？",
      "Text": "……讓我來親眼見證吧。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "目白阿爾丹",
      "Text": "──訓練員。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "目白阿爾丹",
      "Text": "在比賽開始前，\n想請你聽我說一句話。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "目白阿爾丹",
      "Text": "有你當我的訓練員，我──",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "目白阿爾丹",
      "Text": "覺得很幸福。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "目白阿爾丹",
      "Text": "請你親眼見證。\n我和你一起累積的「現在」，\n將早已注定的歷史改變的瞬間──",
      "ChoiceDataList": [
        "去吧"
      ],
      "ColorTextInfoList": []
    },
    {
      "Name": "目白阿爾丹",
      "Text": "是。\n我出發了。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "目白阿爾丹",
      "Text": "（……這條路好像很漫長，\n卻又彷彿只有一剎那。）",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "目白阿爾丹",
      "Text": "（不過我的心裡，\n卻擁有多如繁星的「現在」。）",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "目白阿爾丹",
      "Text": "（值得敬愛的勁敵們，\n給予了我永不褪色的閃耀時光──）",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "目白阿爾丹",
      "Text": "（以及一同超越極限，\n那每場精彩比賽的記憶──）",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "目白阿爾丹",
      "Text": "（我將把這一切毫無遺漏地，\n化為我「現在」的光輝。）",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "千代王＆八重",
      "Text": "──阿爾丹。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "八重無敵",
      "Text": "開始吧。",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "櫻花千代王",
      "Text": "屬於我們的──",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    },
    {
      "Name": "目白阿爾丹",
      "Text": "──「天皇賞（秋）」！",
      "ChoiceDataList": [],
      "ColorTextInfoList": []
    }
  ]
